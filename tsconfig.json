{
  "compilerOptions": {
    "sourceMap": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "lib": ["es2015", "dom", "es2017"],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": true,
    "baseUrl": "./",
    "jsx": "preserve",
    "esModuleInterop": true
  }
}
